  },
  "dependencies": {
    "@rock-js/tools": "^0.9.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@rock-js/config": "^0.9.1"
  },
  "peerDependencies": {
    "@callstack/repack": ">=5.2.0",
    "@rspack/core": ">=1.4.11",
    "@swc/helpers": ">=0.5.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
