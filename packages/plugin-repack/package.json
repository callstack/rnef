{
  "name": "@rock-js/plugin-repack",
  "version": "0.9.1",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "types": "./dist/src/index.d.ts",
    "default": "./dist/src/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
