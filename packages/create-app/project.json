{
  "name": "create-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/create-app/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "packages/create-app/dist",
        "main": "packages/create-app/src/bin.ts",
        "tsConfig": "packages/create-app/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/src/*.{ts,js,tsx,jsx}"]
      }
    },
    "e2e": {
      "dependsOn": [
        "build",
        "plugin-platform-apple:build",
        "plugin-platform-ios:build",
        "plugin-platform-android:build"
      ]
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
