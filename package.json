{
  "name": "rnef",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest run",
    "e2e": "pnpm -r run e2e",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "watch": "pnpm -r --parallel --stream run dev",
    "dev": "pnpm -r --parallel --stream run dev",
    "validate": "pnpm typecheck && pnpm lint && pnpm test",
    "link-packages": "node ./scripts/linkPackages.mjs",
    "version": "changeset version && ./scripts/update-template-dependencies.sh && pnpm install --no-frozen-lockfile",
    "publish:npm": "./scripts/npm-publish.sh",
    "verdaccio:init": "node ./scripts/verdaccio-init.mjs",
    "verdaccio:publish": "node ./scripts/verdaccio-publish.mjs",
    "verdaccio:reset": "./scripts/verdaccio-reset.sh"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.3.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@clack/prompts": "^0.10.0",
    "@eslint/js": "^9.8.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^18.19.68",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript-eslint/utils": "^8.13.0",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fast-glob": "^3.3.2",
    "jsonc-eslint-parser": "^2.4.0",
    "nano-spawn": "^0.2.0",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.13.0",
    "verdaccio": "^6.0.5",
    "vite": "^5.0.0",
    "vitest": "^2.1.2"
  },
  "packageManager": "pnpm@10.11.0"
}
